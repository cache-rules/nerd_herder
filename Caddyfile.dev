0.0.0.0:8080 {
    tls self_signed
    gzip
    root /var/www/nerd_herder/static

    proxy /api nerd_herder:8080 {
        transparent
    }

    proxy /admin nerd_herder:8080 {
        transparent
    }

    proxy /code-of-conduct nerd_herder:8080 {
        transparent
    }

    proxy /static/admin nerd_herder:8080 {
        transparent
    }

    proxy /static/rest_framework nerd_herder:8080 {
        transparent
    }

    proxy / frontend:3000 {
        transparent
        websocket
    }
}
